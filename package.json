{
	"name": "torch-vis",
	"displayName": "Torch Model Visualizer",
	"description": "Visualization Tool for Torch Models based on Netron",
	"version": "0.0.1",
	"publisher": "chingweihsu0809",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/chingweihsu0809/TorchVSCodeVisTool"
	},
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "torchVis.plot",
				"displayName": "Plot Torch Model",
				"selector": [
					{
						"filenamePattern": "*.pt"
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -w -p ./"
	},
	"devDependencies": {
		"@types/node": "^16.11.7",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"eslint": "^8.26.0",
		"typescript": "^4.8.4"
	},
	"dependencies": {
		"python-bridge": "^1.1.0"
	}
}
